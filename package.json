{
  "name": "tradingview-chart-analysis",
  "version": "1.0.0",
  "description": "Manual TradingView chart analysis using OpenAI Vision API for Indian NSE stocks",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run serve:static\"",
    "dev:server": "nodemon --exec ts-node trading-server.ts",
    "dev:unified": "nodemon --exec ts-node unified-trading-server.ts",
    "serve:static": "http-server . -p 5173 -c-1",
    "build": "npm run build:server",
    "build:server": "tsc",
    "start": "node dist/server.js",
    
    "login:url": "node get-login-url.js",
    "login:token": "node get-access-token.js",
    "login:auto": "node auto-login.js",
    
    "analyze:single": "node data-driven-llm-predictor.js",
    "analyze:compare": "node system-comparison.js",
    "analyze:patterns": "node pattern-recognition-engine.js",
    "analyze:decision": "node trading-decision-framework.js",
    "analyze:monitor": "node real-time-data-analyzer.js",
    "analyze:realtime": "node realtime-llm-trading-system.js",
    "analyze:dual": "node dual-system-comparison.js",
    
    "analyze:real": "node analyze-real.js",
    "analyze:llm": "node analyze-full-llm.js",
    "analyze:enhanced": "node analyze-enhanced-profile-cpr.js",
    
    "trading:quick": "npm run login:token && npm run analyze:compare",
    "trading:full": "npm run login:token && npm run analyze:monitor",
    "trading:single": "npm run login:token && npm run analyze:single",
    "trading:realtime": "npm run login:token && npm run analyze:realtime",
    "trading:dual": "npm run login:token && npm run analyze:dual",
    
    "data:status": "node data-manager.js status",
    "data:signals": "node data-manager.js signals",
    "data:stocks": "node data-manager.js stocks",
    "data:backup": "node data-manager.js backup",
    "data:clean": "node data-manager.js clean",
    "data:info": "node data-manager.js info",
    
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "trading",
    "chart-analysis",
    "ai",
    "openai",
    "vision",
    "indian-market",
    "nse",
    "tradingview"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.1.0",
    "kiteconnect": "^5.1.0",
    "moment-timezone": "^0.5.43",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.104.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.17",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
